{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "@mate-academy/eslint-config-react-typescript",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": "off",
    "import/no-extraneous-dependencies": "off",
    "react/jsx-no-bind": "off"
  }
}
